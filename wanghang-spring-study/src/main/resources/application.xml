<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" default-autowire="byName">

	<bean id="person" class="org.wanghang.springframework.Person"/>


	<bean id="order" class="org.wanghang.springframework.beanorder.Order">
		<property name="orderId" value="100"/>
		<property name="orderName" value="wanghangOrder100"/>
	</bean>

	<bean id="orderBeanFactoryPostProcessor" class="org.wanghang.springframework.beanorder.OrderBeanFactoryPostProcessor"/>
	<bean id="orderBeanPostProcessor" class="org.wanghang.springframework.beanorder.OrderBeanPostProcessor"/>
</beans>